{
	"name": "@pie-players/local-esm-cdn",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./embedded": {
			"types": "./dist/embedded.d.ts",
			"default": "./dist/embedded.js"
		},
		"./adapters/vite": {
			"types": "./dist/adapters/vite.d.ts",
			"default": "./dist/adapters/vite.js"
		},
		"./adapters/sveltekit": {
			"types": "./dist/adapters/sveltekit.d.ts",
			"default": "./dist/adapters/sveltekit.js"
		},
		"./adapters/connect": {
			"types": "./dist/adapters/connect.d.ts",
			"default": "./dist/adapters/connect.js"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"dev": "bun run src/index.ts",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"es-module-lexer": "^2.0.0"
	},
	"peerDependencies": {
		"vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
		"@sveltejs/kit": "^2.51.0"
	},
	"peerDependenciesMeta": {
		"vite": {
			"optional": true
		},
		"@sveltejs/kit": {
			"optional": true
		}
	},
	"devDependencies": {
		"@types/node": "^25.0.10",
		"bun-types": "^1.3.6"
	},
	"license": "MIT"
}
