    "pie-packages:fixed-player-build-and-test-package": {
      "aliases": [],
      "args": {},
      "description": "Build a pie-fixed-player-static package (optionally publish) and optionally generate a browser test project",
      "examples": [
        "$ bun run cli pie-packages:fixed-player-build-and-test-package --elements-file configs/fixed-player-static/example.json --generate-test-project",
        "$ bun run cli pie-packages:fixed-player-build-and-test-package -e \"@pie-element/multiple-choice@11.4.3\" --publish --dry-run"
      ],
      "flags": {
        "elementsFile": {
          "char": "f",
          "description": "Path to elements JSON file",
          "exclusive": [
            "elements"
          ],
          "name": "elementsFile",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "elements": {
          "char": "e",
          "description": "Comma-separated list of elements (package@version)",
          "exclusive": [
            "elementsFile"
          ],
          "name": "elements",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "iteration": {
          "char": "i",
          "description": "Iteration number for package versioning (used for version string)",
          "name": "iteration",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "loaderVersion": {
          "description": "Override loader version used in the static package version string (default: pie-fixed-player version in this repo)",
          "name": "loaderVersion",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "overwriteBundle": {
          "description": "Force rebuild of bundle even if it exists in cache (adds ?overwrite=true)",
          "name": "overwriteBundle",
          "allowNo": false,
          "type": "boolean"
        },
        "publish": {
          "char": "p",
          "description": "Publish package to npm after building",
          "name": "publish",
          "allowNo": false,
          "type": "boolean"
        },
        "dryRun": {
          "description": "Dry run (build but do not publish)",
          "name": "dryRun",
          "allowNo": false,
          "type": "boolean"
        },
        "pitsBaseUrl": {
          "description": "Bundle builder base URL (default: https://proxy.pie-api.com)",
          "name": "pitsBaseUrl",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "generateTestProject": {
          "char": "g",
          "description": "Generate a browser test project after building",
          "name": "generateTestProject",
          "allowNo": false,
          "type": "boolean"
        },
        "outputDir": {
          "char": "o",
          "description": "Output directory for the generated test project",
          "name": "outputDir",
          "default": "~/Downloads/pie-tag-test-projects",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "name": {
          "char": "n",
          "description": "Test project name (directory name)",
          "name": "name",
          "default": "pie-fixed-player-static-test",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "start": {
          "char": "s",
          "description": "Start a simple static server (python3 http.server) after generation",
          "name": "start",
          "allowNo": false,
          "type": "boolean"
        },
        "port": {
          "description": "Port for the static server (only used with --start)",
          "name": "port",
          "default": 4173,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "pie-packages:fixed-player-build-and-test-package",
      "pluginAlias": "@pie-players/pie-players-cli",
      "pluginName": "@pie-players/pie-players-cli",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "commands",
        "pie-packages",
        "fixed-player-build-and-test-package.js"
      ]
    },
